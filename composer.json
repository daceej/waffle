{
    "name": "daceej/waffle",
    "description": "A tool to prevent waffling!",
    "type": "project",
    "authors": [
        {
            "name": "Curtis Ogle",
            "email": "curtis.jams.ogle@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bats-core/bats-core",
                "version": "1.3.0",
                "dist": {
                    "url": "https://github.com/bats-core/bats-core/archive/refs/tags/v1.3.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "symfony/console": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/process": "^5.1",
        "ext-json": "*",
        "symfony/finder": "^5.1",
        "symfony/dependency-injection": "^5.2",
        "symfony/config": "^5.2",
        "consolidation/self-update": "^1.2"
    },
    "bin": [
        "bin/waffle",
        "bin/wfl"
    ],
    "autoload": {
        "psr-4": {
            "Waffle\\": "src"
        },
        "classmap": [
            "src"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "bats-core/bats-core": "^1.3"
    }
}
